PREFIX schema: <https://schema.org/>

<https://linked.data.gov.au/def/data-roles>
    schema:description "This vocabulary lists the roles that Agents - People and Organisations - play in relation to data. This is an extended and Semantic Web version of the International Organisation for Standardization's ISO19115-1 standard's Role Codes codelist."@en ;
    schema:name "IDN Role Codes"@en ;
.

<https://linked.data.gov.au/def/data-roles/author>
    schema:description "party who authored the resource" ;
    schema:name "author"@en ;
.

<https://linked.data.gov.au/def/data-roles/coAuthor>
    schema:description "party who jointly authors the resource" ;
    schema:name "co author"@en ;
.

<https://linked.data.gov.au/def/data-roles/collaborator>
    schema:description "party who assists with the generation of the resource other than the principal investigator" ;
    schema:name "collaborator"@en ;
.

<https://linked.data.gov.au/def/data-roles/contributor>
    schema:description "party contributing to the resource" ;
    schema:name "contributor"@en ;
.

<https://linked.data.gov.au/def/data-roles/custodian>
    schema:description "party that accepts accountability and responsibility for the resource and ensures appropriate care and maintenance of the resource" ;
    schema:name "custodian"@en ;
.

<https://linked.data.gov.au/def/data-roles/distributor>
    schema:description "party who distributes the resource" ;
    schema:name "distributor"@en ;
.

<https://linked.data.gov.au/def/data-roles/editor>
    schema:description "party who reviewed or modified the resource to improve the content" ;
    schema:name "editor"@en ;
.

<https://linked.data.gov.au/def/data-roles/funder>
    schema:description "party providing monetary support for the resource" ;
    schema:name "funder"@en ;
.

<https://linked.data.gov.au/def/data-roles/isoRoles>
    schema:description "Role Codes presented in the ISO 19115 original standard's codelist"@en ;
    schema:name "ISO CI Role Code codes"@en ;
.

<https://linked.data.gov.au/def/data-roles/mediator>
    schema:description "a class of entity that mediates access to the resource and for whom the resource is intended or useful" ;
    schema:name "mediator"@en ;
.

<https://linked.data.gov.au/def/data-roles/originator>
    schema:description "party who created the resource" ;
    schema:name "originator"@en ;
.

<https://linked.data.gov.au/def/data-roles/owner>
    schema:description "party that owns the resource" ;
    schema:name "owner"@en ;
.

<https://linked.data.gov.au/def/data-roles/pointOfContact>
    schema:description "party who can be contacted for acquiring knowledge about or acquisition of the resource" ;
    schema:name "point of contact"@en ;
.

<https://linked.data.gov.au/def/data-roles/principalInvestigator>
    schema:description "key party responsible for gathering information and conducting research" ;
    schema:name "principal investigator"@en ;
.

<https://linked.data.gov.au/def/data-roles/processor>
    schema:description "party who has processed the data in a manner such that the resource has been modified" ;
    schema:name "processor"@en ;
.

<https://linked.data.gov.au/def/data-roles/publisher>
    schema:description "party who published the resource" ;
    schema:name "publisher"@en ;
.

<https://linked.data.gov.au/def/data-roles/resourceProvider>
    schema:description "party that supplies the resource" ;
    schema:name "resource provider"@en ;
.

<https://linked.data.gov.au/def/data-roles/rightsHolder>
    schema:description "party owning or managing rights over the resource" ;
    schema:name "rights holder"@en ;
.

<https://linked.data.gov.au/def/data-roles/sponsor>
    schema:description "party who speaks for the resource" ;
    schema:name "sponsor"@en ;
.

<https://linked.data.gov.au/def/data-roles/stakeholder>
    schema:description "party who has an interest in the resource or the use of the resource" ;
    schema:name "stakeholder"@en ;
.

<https://linked.data.gov.au/def/data-roles/subjectAgent>
    schema:description "party that the resources contains information about" ;
    schema:name "subject agent"@en ;
.

<https://linked.data.gov.au/def/data-roles/subjectAgentRepresentative>
    schema:description "party who can be contacted who best represents the interests of the subject agent" ;
    schema:name "subject agent representative"@en ;
.

<https://linked.data.gov.au/def/data-roles/user>
    schema:description "party who uses the resource" ;
    schema:name "user"@en ;
.

<https://linked.data.gov.au/org/agldwg>
    schema:description "The Australian Government's community of practice for Linked Data"@en ;
    schema:name "Australian Government Linked Data Working Group" ;
.

<https://linked.data.gov.au/org/idn>
    schema:description "The Indigenous Data Network (IDN) was established in 2018 to support and coordinate the governance of Indigenous data for Aboriginal and Torres Strait Islander peoples and empower Aboriginal and Torres Strait Islander communities to decide their own local data priorities."@en ;
    schema:name "Indigenous Data Network" ;
.
