PREFIX schema: <https://schema.org/>

<https://linked.data.gov.au/def/addr>
    schema:description "This model is a Semantic Web data model used for address information. It caters specifically for Australian & New Zealand's address modelling needs."@en ;
    schema:name "Address Model"@en ;
.

<https://linked.data.gov.au/def/addr/Address>
    schema:description "The Address class represents structured information that allows unambiguous determination of an object for the purposes of identification and location"@en ;
    schema:name "Address"@en ;
.

<https://linked.data.gov.au/def/addr/AddressComponent>
    schema:description "A component that is a constituent part of an address"@en ;
    schema:name "Address Component"@en ;
.

<https://linked.data.gov.au/def/addr/AddressComponentType>
    schema:description "Code that specifies the kind of address component"@en ;
    schema:name "Address Component Type"@en ;
.

<https://linked.data.gov.au/def/addr/AddressLifecycleStage>
    schema:description "Represents the different lifecycle stages of an Address"@en ;
    schema:name "Address Lifecycle Stage"@en ;
.

<https://linked.data.gov.au/def/addr/AddressLifecycleStageType>
    schema:description "Code that specifies the kind of Address Lifecycle Stage"@en ;
    schema:name "Address Lifecycle Stage Type"@en ;
.

<https://linked.data.gov.au/def/addr/AddressRole>
    schema:description "AddressRole represents a task for which this Address may be used"@en ;
    schema:name "Address Role"@en ;
.

<https://linked.data.gov.au/def/addr/AddressableObject>
    schema:description """An object that is unambiguously determined by an address.

Examples of types of AddressableObjects are a building, a dwelling or a land parcel."""@en ;
    schema:name "Addressable Object"@en ;
.

<https://linked.data.gov.au/def/addr/hasAddress>
    schema:description "Indicates an Address has been allocated for an Addressable Object"@en ;
    schema:name "has address"@en ;
.

<https://linked.data.gov.au/def/addr/hasAddressComponent>
    schema:description "Indicates an Address Component of an Address"@en ;
    schema:name "has address component"@en ;
.

<https://linked.data.gov.au/def/addr/hasAddressComponentType>
    schema:description "Indicates an Addresses Component's type"@en ;
    schema:name "has address component type"@en ;
.

<https://linked.data.gov.au/def/addr/hasAddressRole>
    schema:description "Indicates an Address Role for an Address"@en ;
    schema:name "has address role"@en ;
.

<https://linked.data.gov.au/def/addr/hasGeocode>
    schema:description "Indicates a refined, that is a very accurate or specific, geometry, usually a point, for an Address qualified by the Geocode Type - how it was generated."@en ;
    schema:name "has geocode"@en ;
.

<https://linked.data.gov.au/def/addr/hasLifecycleStage>
    schema:description "Indicates an Addresses' Lifecycle Stage"@en ;
    schema:name "has lifecycle stage"@en ;
.

<https://linked.data.gov.au/def/addr/hasValue>
    schema:description "Indicates the value of an Address Component"@en ;
    schema:name "has value"@en ;
.

<https://linked.data.gov.au/def/addr/hasValueText>
    schema:description "Indicates the value of an Address Component"@en ;
    schema:name "has value text"@en ;
.

<https://linked.data.gov.au/def/addr/isAddressFor>
    schema:description "Indicates an Addressable Object that an Address is allocated to"@en ;
    schema:name "is address for"@en ;
.

<https://linked.data.gov.au/org/qsi>
    schema:name "Spatial Information Queensland"@en ;
.

<https://orcid.org/0000-0002-8742-7730>
    schema:name "Nicholas J. Car"@en ;
.
