PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX schema: <https://schema.org/>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>

<http://purl.org/ontology/bibo>
    schema:description """The Bibliographic Ontology describes
bibliographic things on the semantic Web in RDF.  This ontology can be
used as a citation ontology, as a document classification ontology, or
simply as a way to describe any kind of document in RDF. It has been
inspired by many existing document description metadata formats, and
can be used as a common ground for converting other bibliographic data
sources."""@en ;
    schema:name "The Bibliographic Ontology" ;
.

<http://purl.org/ontology/bibo/AcademicArticle>
    schema:description "A scholarly academic article, typically published in a journal."@en ;
    schema:name "Academic Article"@en ;
.

<http://purl.org/ontology/bibo/Article>
    schema:description "A written composition in prose, usually nonfiction, on a specific topic, forming an independent part of a book or other publication, as a newspaper or magazine."@en ;
    schema:name "Article"@en ;
.

<http://purl.org/ontology/bibo/AudioDocument>
    schema:description "An audio document; aka record."@en ;
    schema:name "audio document"@en ;
.

<http://purl.org/ontology/bibo/AudioVisualDocument>
    schema:description "An audio-visual document; film, video, and so forth."@en ;
    schema:name "audio-visual document"@en ;
.

<http://purl.org/ontology/bibo/Bill>
    schema:description "Draft legislation presented for discussion to a legal body."@en ;
    schema:name "Bill"@en ;
.

<http://purl.org/ontology/bibo/Book>
    schema:description "A written or printed work of fiction or nonfiction, usually on sheets of paper fastened or bound together within covers."@en ;
    schema:name "Book"@en ;
.

<http://purl.org/ontology/bibo/BookSection>
    schema:description "A section of a book."@en ;
    schema:name "Book Section"@en ;
.

<http://purl.org/ontology/bibo/Brief>
    schema:description "A written argument submitted to a court."@en ;
    schema:name "Brief"@en ;
.

<http://purl.org/ontology/bibo/Chapter>
    schema:description "A chapter of a book."@en ;
    schema:name "Chapter"@en ;
.

<http://purl.org/ontology/bibo/Code>
    schema:description "A collection of statutes."@en ;
    schema:name "Code"@en ;
.

<http://purl.org/ontology/bibo/CollectedDocument>
    schema:description "A document that simultaneously contains other documents."@en ;
    schema:name "Collected Document"@en ;
.

<http://purl.org/ontology/bibo/Collection>
    schema:description "A collection of Documents or Collections"@en ;
    schema:name "Collection"@en ;
.

<http://purl.org/ontology/bibo/Conference>
    schema:description "A meeting for consultation or discussion."@en ;
    schema:name "Conference"@en ;
.

<http://purl.org/ontology/bibo/CourtReporter>
    schema:description "A collection of legal cases."@en ;
    schema:name "Court Reporter"@en ;
.

<http://purl.org/ontology/bibo/Document>
    schema:description "A document (noun) is a bounded physical representation of body of information designed with the capacity (and usually intent) to communicate. A document may manifest symbolic, diagrammatic or sensory-representational information."@en ;
    schema:name "Document"@en ;
.

<http://purl.org/ontology/bibo/DocumentPart>
    schema:description "a distinct part of a larger document or collected document."@en ;
    schema:name "document part"@en ;
.

<http://purl.org/ontology/bibo/DocumentStatus>
    schema:description "The status of the publication of a document."@en ;
    schema:name "Document Status"@en ;
.

<http://purl.org/ontology/bibo/EditedBook>
    schema:description "An edited book."@en ;
    schema:name "Edited Book"@en ;
.

<http://purl.org/ontology/bibo/Email>
    schema:description "A written communication addressed to a person or organization and transmitted electronically."@en ;
    schema:name "EMail"@en ;
.

<http://purl.org/ontology/bibo/Excerpt>
    schema:description "A passage selected from a larger work."@en ;
    schema:name "Excerpt"@en ;
.

<http://purl.org/ontology/bibo/Film>
    schema:description "aka movie."@en ;
    schema:name "Film"@en ;
.

<http://purl.org/ontology/bibo/Hearing>
    schema:description "An instance or a session in which testimony and arguments are presented, esp. before an official, as a judge in a lawsuit."@en ;
    schema:name "Hearing"@en ;
.

<http://purl.org/ontology/bibo/Image>
    schema:description "A document that presents visual or diagrammatic information."@en ;
    schema:name "Image"@en ;
.

<http://purl.org/ontology/bibo/Interview>
    schema:description "A formalized discussion between two or more people."@en ;
    schema:name "Interview"@en ;
.

<http://purl.org/ontology/bibo/Issue>
    schema:description "something that is printed or published and distributed, esp. a given number of a periodical"@en ;
    schema:name "Issue"@en ;
.

<http://purl.org/ontology/bibo/Journal>
    schema:description "A periodical of scholarly journal Articles."@en ;
    schema:name "Journal"@en ;
.

<http://purl.org/ontology/bibo/LegalCaseDocument>
    schema:description "A document accompanying a legal case."@en ;
    schema:name "Legal Case Document"@en ;
.

<http://purl.org/ontology/bibo/LegalDecision>
    schema:description "A document containing an authoritative determination (as a decree or judgment) made after consideration of facts or law."@en ;
    schema:name "Decision"@en ;
.

<http://purl.org/ontology/bibo/LegalDocument>
    schema:description "A legal document; for example, a court decision, a brief, and so forth."@en ;
    schema:name "Legal Document"@en ;
.

<http://purl.org/ontology/bibo/Legislation>
    schema:description "A legal document proposing or enacting a law or a group of laws."@en ;
    schema:name "Legislation"@en ;
.

<http://purl.org/ontology/bibo/Letter>
    schema:description "A written or printed communication addressed to a person or organization and usually transmitted by mail."@en ;
    schema:name "Letter"@en ;
.

<http://purl.org/ontology/bibo/Magazine>
    schema:description "A periodical of magazine Articles. A magazine is a publication that is issued periodically, usually bound in a paper cover, and typically contains essays, stories, poems, etc., by many writers, and often photographs and drawings, frequently specializing in a particular subject or area, as hobbies, news, or sports."@en ;
    schema:name "Magazine"@en ;
.

<http://purl.org/ontology/bibo/Manual>
    schema:description "A small reference book, especially one giving instructions."@en ;
    schema:name "Manual"@en ;
.

<http://purl.org/ontology/bibo/Manuscript>
    schema:description "An unpublished Document, which may also be submitted to a publisher for publication."@en ;
    schema:name "Manuscript"@en ;
.

<http://purl.org/ontology/bibo/Map>
    schema:description "A graphical depiction of geographic features."@en ;
    schema:name "Map"@en ;
.

<http://purl.org/ontology/bibo/MultiVolumeBook>
    schema:description "A loose, thematic, collection of Documents, often Books."@en ;
    schema:name "Multivolume Book"@en ;
.

<http://purl.org/ontology/bibo/Newspaper>
    schema:description "A periodical of documents, usually issued daily or weekly, containing current news, editorials, feature articles, and usually advertising."@en ;
    schema:name "Newspaper"@en ;
.

<http://purl.org/ontology/bibo/Note>
    schema:description "Notes or annotations about a resource."@en ;
    schema:name "Note"@en ;
.

<http://purl.org/ontology/bibo/Patent>
    schema:description "A document describing the exclusive right granted by a government to an inventor to manufacture, use, or sell an invention for a certain number of years."@en ;
    schema:name "Patent"@en ;
.

<http://purl.org/ontology/bibo/Performance>
    schema:description "A public performance."@en ;
    schema:name "Performance"@en ;
.

<http://purl.org/ontology/bibo/Periodical>
    schema:description "A group of related documents issued at regular intervals."@en ;
    schema:name "Periodical"@en ;
.

<http://purl.org/ontology/bibo/PersonalCommunication>
    schema:description "A communication between an agent and one or more specific recipients."@en ;
    schema:name "Personal Communication"@en ;
.

<http://purl.org/ontology/bibo/PersonalCommunicationDocument>
    schema:description "A personal communication manifested in some document."@en ;
    schema:name "Personal Communication Document"@en ;
.

<http://purl.org/ontology/bibo/Proceedings>
    schema:description "A compilation of documents published from an event, such as a conference."@en ;
    schema:name "Proceedings"@en ;
.

<http://purl.org/ontology/bibo/Quote>
    schema:description "An excerpted collection of words."@en ;
    schema:name "Quote"@en ;
.

<http://purl.org/ontology/bibo/ReferenceSource>
    schema:description "A document that presents authoritative reference information, such as a dictionary or encylopedia ."@en ;
    schema:name "Reference Source"@en ;
.

<http://purl.org/ontology/bibo/Report>
    schema:description "A document describing an account or statement describing in detail an event, situation, or the like, usually as the result of observation, inquiry, etc.."@en ;
    schema:name "Report"@en ;
.

<http://purl.org/ontology/bibo/Series>
    schema:description "A loose, thematic, collection of Documents, often Books."@en ;
    schema:name "Series"@en ;
.

<http://purl.org/ontology/bibo/Slide>
    schema:description "A slide in a slideshow"@en ;
    schema:name "Slide"@en ;
.

<http://purl.org/ontology/bibo/Slideshow>
    schema:description "A presentation of a series of slides, usually presented in front of an audience with written text and images."@en ;
    schema:name "Slideshow"@en ;
.

<http://purl.org/ontology/bibo/Specification>
    schema:description "A document describing a specification."@en ;
    schema:name "Specification"@en ;
.

<http://purl.org/ontology/bibo/Standard>
    schema:description "A document describing a standard: a specification organized through a standards body."@en ;
    schema:name "Standard"@en ;
.

<http://purl.org/ontology/bibo/Statute>
    schema:description "A bill enacted into law."@en ;
    schema:name "Statute"@en ;
.

<http://purl.org/ontology/bibo/Thesis>
    schema:description "A document created to summarize research findings associated with the completion of an academic degree."@en ;
    schema:name "Thesis"@en ;
.

<http://purl.org/ontology/bibo/ThesisDegree>
    schema:description "The academic degree of a Thesis"@en ;
    schema:name "Thesis degree"@en ;
.

<http://purl.org/ontology/bibo/Webpage>
    schema:description "A web page is an online document available (at least initially) on the world wide web. A web page is written first and foremost to appear on the web, as distinct from other online resources such as books, manuscripts or audio documents which use the web primarily as a distribution mechanism alongside other more traditional methods such as print."@en ;
    schema:name "Webpage"@en ;
.

<http://purl.org/ontology/bibo/Website>
    schema:description "A group of Webpages accessible on the Web."@en ;
    schema:name "Website"@en ;
.

<http://purl.org/ontology/bibo/Workshop>
    schema:description "A seminar, discussion group, or the like, that emphasizes zxchange of ideas and the demonstration and application of techniques, skills, etc."@en ;
    schema:name "Workshop"@en ;
.

<http://purl.org/ontology/bibo/abstract>
    schema:description "A summary of the resource." ;
    schema:name "abstract" ;
.

<http://purl.org/ontology/bibo/affirmedBy>
    schema:description "A legal decision that affirms a ruling."@en ;
.

<http://purl.org/ontology/bibo/annotates>
    schema:description "Critical or explanatory note for a Document."@en ;
    schema:name "annotates"@en ;
.

<http://purl.org/ontology/bibo/argued>
    schema:description "The date on which a legal case is argued before a court. Date is of format xsd:date"@en ;
    schema:name "date argued"@en ;
.

<http://purl.org/ontology/bibo/authorList>
    schema:description "An ordered list of authors. Normally, this list is seen as a priority list that order authors by importance."@en ;
    schema:name "list of authors"@en ;
.

<http://purl.org/ontology/bibo/bdarcus>
    rdfs:seeAlso "http://purl.org/net/darcusb/info#me"^^xsd:anyURI ;
.

<http://purl.org/ontology/bibo/chapter>
    schema:description "An chapter number"@en ;
    schema:name "chapter"@en ;
.

<http://purl.org/ontology/bibo/citedBy>
    schema:description """Relates a document to another document that cites the
first document."""@en ;
    schema:name "cited by"@en ;
.

<http://purl.org/ontology/bibo/cites>
    schema:description """Relates a document to another document that is cited
by the first document as reference, comment, review, quotation or for
another purpose."""@en ;
    schema:name "cites"@en ;
.

<http://purl.org/ontology/bibo/content>
    schema:description "This property is for a plain-text rendering of the content of a Document. While the plain-text content of an entire document could be described by this property."@en ;
    schema:name "content"@en ;
.

<http://purl.org/ontology/bibo/contributorList>
    schema:description "An ordered list of contributors. Normally, this list is seen as a priority list that order contributors by importance."@en ;
    schema:name "list of contributors"@en ;
.

<http://purl.org/ontology/bibo/court>
    schema:description "A court associated with a legal document; for example, that which issues a decision."@en ;
    schema:name "court"@en ;
.

<http://purl.org/ontology/bibo/degree>
    schema:description "The thesis degree."@en ;
    schema:name "degree"@en ;
.

<http://purl.org/ontology/bibo/degrees/ma>
    schema:description "masters degree in arts"@en ;
    schema:name "M.A."@en ;
.

<http://purl.org/ontology/bibo/degrees/ms>
    schema:description "masters degree in science"@en ;
    schema:name "M.S."@en ;
.

<http://purl.org/ontology/bibo/degrees/phd>
    schema:description "PhD degree"@en ;
    schema:name "PhD degree"@en ;
.

<http://purl.org/ontology/bibo/director>
    schema:description "A Film director."@en ;
    schema:name "director" ;
.

<http://purl.org/ontology/bibo/distributor>
    schema:description "Distributor of a document or a collection of documents."@en ;
    schema:name "distributor"@en ;
.

<http://purl.org/ontology/bibo/edition>
    schema:description "The name defining a special edition of a document. Normally its a literal value composed of a version number and words."@en ;
    schema:name "edition"@en ;
.

<http://purl.org/ontology/bibo/editor>
    schema:description "A person having managerial and sometimes policy-making responsibility for the editorial part of a publishing firm or of a newspaper, magazine, or other publication."@en ;
    schema:name "editor" ;
.

<http://purl.org/ontology/bibo/editorList>
    schema:description "An ordered list of editors. Normally, this list is seen as a priority list that order editors by importance."@en ;
    schema:name "list of editors"@en ;
.

<http://purl.org/ontology/bibo/fgiasson>
    rdfs:seeAlso "http://fgiasson.com/me/"^^xsd:anyURI ;
.

<http://purl.org/ontology/bibo/interviewee>
    schema:description "An agent that is interviewed by another agent."@en ;
    schema:name "interviewee" ;
.

<http://purl.org/ontology/bibo/interviewer>
    schema:description "An agent that interview another agent."@en ;
    schema:name "interviewer" ;
.

<http://purl.org/ontology/bibo/issue>
    schema:description "An issue number"@en ;
    schema:name "issue"@en ;
.

<http://purl.org/ontology/bibo/issuer>
    schema:description "An entity responsible for issuing often informally published documents such as press releases, reports, etc." ;
    schema:name "issuer" ;
.

<http://purl.org/ontology/bibo/locator>
    schema:description "A description (often numeric) that locates an item within a containing document or collection."@en ;
    schema:name "locator"@en ;
.

<http://purl.org/ontology/bibo/numPages>
    schema:description "The number of pages contained in a document"@en ;
    schema:name "number of pages"@en ;
.

<http://purl.org/ontology/bibo/numVolumes>
    schema:description "The number of volumes contained in a collection of documents (usually a series, periodical, etc.)."@en ;
    schema:name "number of volumes"@en ;
.

<http://purl.org/ontology/bibo/number>
    schema:description "A generic item or document number. Not to be confused with issue number."@en ;
    schema:name "number"@en ;
.

<http://purl.org/ontology/bibo/organizer>
    schema:description "The organizer of an event; includes conference organizers, but also government agencies or other bodies that are responsible for conducting hearings."@en ;
    schema:name "organizer"@en ;
.

<http://purl.org/ontology/bibo/owner>
    schema:description "Owner of a document or a collection of documents."@en ;
    schema:name "owner"@en ;
.

<http://purl.org/ontology/bibo/pageEnd>
    schema:description "Ending page number within a continuous page range."@en ;
    schema:name "page end"@en ;
.

<http://purl.org/ontology/bibo/pageStart>
    schema:description "Starting page number within a continuous page range."@en ;
    schema:name "page start"@en ;
.

<http://purl.org/ontology/bibo/pages>
    schema:description "A string of non-contiguous page spans that locate a Document within a Collection. Example: 23-25, 34, 54-56. For continuous page ranges, use the pageStart and pageEnd properties."@en ;
    schema:name "pages"@en ;
.

<http://purl.org/ontology/bibo/performer>
    schema:name "performer" ;
.

<http://purl.org/ontology/bibo/prefixName>
    schema:description "The prefix of a name"@en ;
    schema:name "prefix name"@en ;
.

<http://purl.org/ontology/bibo/presentedAt>
    schema:description "Relates a document to an event; for example, a paper to a conference."@en ;
    schema:name "presented at"@en ;
.

<http://purl.org/ontology/bibo/presents>
    schema:description "Relates an event to associated documents; for example, conference to a paper."@en ;
    schema:name "presents"@en ;
.

<http://purl.org/ontology/bibo/producer>
    schema:description "Producer of a document or a collection of documents."@en ;
    schema:name "producer"@en ;
.

<http://purl.org/ontology/bibo/recipient>
    schema:description "An agent that receives a communication document."@en ;
    schema:name "recipient" ;
.

<http://purl.org/ontology/bibo/reproducedIn>
    schema:description "The resource in which another resource is reproduced."@en ;
.

<http://purl.org/ontology/bibo/reversedBy>
    schema:description "A legal decision that reverses a ruling."@en ;
.

<http://purl.org/ontology/bibo/reviewOf>
    schema:description "Relates a review document to a reviewed thing (resource, item, etc.)."@en ;
    schema:name "review of"@en ;
.

<http://purl.org/ontology/bibo/section>
    schema:description "A section number"@en ;
    schema:name "section"@en ;
.

<http://purl.org/ontology/bibo/shortTitle>
    schema:description "The abbreviation of a title."@en ;
    schema:name "short title"@en ;
.

<http://purl.org/ontology/bibo/status>
    schema:description "The publication status of (typically academic) content."@en ;
    schema:name "status"@en ;
.

<http://purl.org/ontology/bibo/status/accepted>
    schema:description "Accepted for publication after peer reviewing."@en ;
    schema:name "accepted"@en ;
.

<http://purl.org/ontology/bibo/status/draft>
    schema:description "Document drafted"@en ;
    schema:name "draft"@en ;
.

<http://purl.org/ontology/bibo/status/forthcoming>
    schema:description "Document to be published"@en ;
    schema:name "forthcoming"@en ;
.

<http://purl.org/ontology/bibo/status/legal>
    schema:description "Legal document"@en ;
    schema:name "legal"@en ;
.

<http://purl.org/ontology/bibo/status/nonPeerReviewed>
    schema:description "A document that is not peer reviewed"@en ;
    schema:name "non peer reviewed"@en ;
.

<http://purl.org/ontology/bibo/status/peerReviewed>
    schema:description "The process by which articles are chosen to be included in a refereed journal. An editorial board consisting of experts in the same field as the author review the article and decide if it is authoritative enough for publication."@en ;
    schema:name "peer reviewed"@en ;
.

<http://purl.org/ontology/bibo/status/published>
    schema:description "Published document"@en ;
    schema:name "published"@en ;
.

<http://purl.org/ontology/bibo/status/rejected>
    schema:description "Rejected for publication after peer reviewing."@en ;
    schema:name "rejected"@en ;
.

<http://purl.org/ontology/bibo/status/unpublished>
    schema:description "Unpublished document"@en ;
    schema:name "unpublished"@en ;
.

<http://purl.org/ontology/bibo/subsequentLegalDecision>
    schema:description "A legal decision on appeal that takes action on a case (affirming it, reversing it, etc.)."@en ;
.

<http://purl.org/ontology/bibo/suffixName>
    schema:description "The suffix of a name"@en ;
    schema:name "suffix name"@en ;
.

<http://purl.org/ontology/bibo/transcriptOf>
    schema:description "Relates a document to some transcribed original."@en ;
    schema:name "transcript of"@en ;
.

<http://purl.org/ontology/bibo/translationOf>
    schema:description "Relates a translated document to the original document."@en ;
    schema:name "translation of"@en ;
.

<http://purl.org/ontology/bibo/translator>
    schema:description "A person who translates written document from one language to another."@en ;
    schema:name "translator" ;
.

<http://purl.org/ontology/bibo/uri>
    schema:description "Universal Resource Identifier of a document"@en ;
    schema:name "uri"@en ;
.

<http://purl.org/ontology/bibo/volume>
    schema:description "A volume number"@en ;
    schema:name "volume"@en ;
.
