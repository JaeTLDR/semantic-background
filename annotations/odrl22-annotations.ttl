odrl:Action
    schema:description "An operation on an Asset."@en ;
    schema:name "Action"@en ;
.

odrl:Agreement
    schema:description "A Policy that grants the assignee a Rule over an Asset from an assigner."@en ;
    schema:name "Agreement"@en ;
.

odrl:All
    schema:description "Specifies that the scope of the relationship is all of the collective individuals within a context."@en ;
    schema:name "All"@en ;
.

odrl:All2ndConnections
    schema:description "Specifies that the scope of the relationship is all of the second-level connections to the Party."@en ;
    schema:name "All Second-level Connections"@en ;
.

odrl:AllConnections
    schema:description "Specifies that the scope of the relationship is all of the first-level connections of the Party."@en ;
    schema:name "All First-Level Connections"@en ;
.

odrl:AllGroups
    schema:description "Specifies that the scope of the relationship is all of the group connections of the Party."@en ;
    schema:name "All Group Connections"@en ;
.

odrl:Assertion
    schema:description "A Policy that asserts a Rule over an Asset from parties."@en ;
    schema:name "Assertion"@en ;
.

odrl:Asset
    schema:description "A resource or a collection of resources that are the subject of a Rule."@en ;
    schema:name "Asset"@en ;
.

odrl:AssetCollection
    schema:description "An Asset that is collection of individual resources"@en ;
    schema:name "Asset Collection"@en ;
.

odrl:AssetScope
    schema:description "Scopes for Asset Scope expressions."@en ;
    schema:name "Asset Scope"@en ;
.

odrl:ConflictTerm
    schema:description "Used to establish strategies to resolve conflicts that arise from the merging of Policies or conflicts between Permissions and Prohibitions in the same Policy."@en ;
    schema:name "Conflict Strategy Preference"@en ;
.

odrl:Constraint
    schema:description "A boolean expression that refines the semantics of an Action and Party/Asset Collection or declare the conditions applicable to a Rule."@en ;
    schema:name "Constraint"@en ;
.

odrl:Duty
    schema:description "The obligation to perform an Action"@en ;
    schema:name "Duty"@en ;
.

odrl:Group
    schema:description "Specifies that the scope of the relationship is the defined group with multiple individual members."@en ;
    schema:name "Group"@en ;
.

odrl:Individual
    schema:description "Specifies that the scope of the relationship is the single Party individual."@en ;
    schema:name "Individual"@en ;
.

odrl:LeftOperand
    schema:description "Left operand for a constraint expression."@en ;
    schema:name "Left Operand"@en ;
.

odrl:LogicalConstraint
    schema:description "A logical expression that refines the semantics of an Action and Party/Asset Collection or declare the conditions applicable to a Rule."@en ;
    schema:name "Logical Constraint"@en ;
.

odrl:Offer
    schema:description "A Policy that proposes a Rule over an Asset from an assigner."@en ;
    schema:name "Offer"@en ;
.

odrl:Operator
    schema:description "Operator for constraint expression."@en ;
    schema:name "Operator"@en ;
.

odrl:Party
    schema:description "An entity or a collection of entities that undertake Roles in a Rule."@en ;
    schema:name "Party"@en ;
.

odrl:PartyCollection
    schema:description "A Party that is a group of individual entities"@en ;
    schema:name "Party Collection"@en ;
.

odrl:PartyScope
    schema:description "Scopes for Party Scope expressions."@en ;
    schema:name "Party Scope"@en ;
.

odrl:Permission
    schema:description "The ability to perform an Action over an Asset."@en ;
    schema:name "Permission"@en ;
.

odrl:Policy
    schema:description "A non-empty group of Permissions and/or Prohibitions."@en ;
    schema:name "Policy"@en ;
.

odrl:Privacy
    schema:description "A Policy that expresses a Rule over an Asset containing personal information."@en ;
    schema:name "Privacy Policy"@en ;
.

odrl:Prohibition
    schema:description "The inability to perform an Action over an Asset."@en ;
    schema:name "Prohibition"@en ;
.

odrl:Request
    schema:description "A Policy that proposes a Rule over an Asset from an assignee."@en ;
    schema:name "Request"@en ;
.

odrl:RightOperand
    schema:description "Right operand for constraint expression."@en ;
    schema:name "Right Operand"@en ;
.

odrl:Rule
    schema:description "An abstract concept that represents the common characteristics of Permissions, Prohibitions, and Duties."@en ;
    schema:name "Rule"@en ;
.

odrl:Set
    schema:description "A Policy that expresses a Rule over an Asset."@en ;
    schema:name "Set"@en ;
.

odrl:Ticket
    schema:description "A Policy that grants the holder a Rule over an Asset from an assigner."@en ;
    schema:name "Ticket"@en ;
.

odrl:UndefinedTerm
    schema:description "Is used to indicate how to support Actions that are not part of any vocabulary or profile in the policy expression system."@en ;
    schema:name "Undefined Term"@en ;
.

odrl:absolutePosition
    schema:description "A point in space or time defined with absolute coordinates for the positioning of the target Asset."@en ;
    schema:name "Absolute Asset Position"@en ;
.

odrl:absoluteSize
    schema:description "Measure(s) of one or two axes for 2D-objects or measure(s) of one to tree axes for 3D-objects of the target Asset."@en ;
    schema:name "Absolute Asset Size"@en ;
.

odrl:absoluteSpatialPosition
    schema:description "The absolute spatial positions of four corners of a rectangle on a 2D-canvas or the eight corners of a cuboid in a 3D-space for the target Asset to fit."@en ;
    schema:name "Absolute Spatial Asset Position"@en ;
.

odrl:absoluteTemporalPosition
    schema:description "The absolute temporal positions in a media stream the target Asset has to fit."@en ;
    schema:name "Absolute Temporal Asset Position"@en ;
.

odrl:acceptTracking
    schema:description "To accept that the use of the Asset may be tracked."@en ;
    schema:name "Accept Tracking"@en ;
.

odrl:action
    schema:description "The operation relating to the Asset for which the Rule is being subjected."@en ;
    schema:name "Has Action"@en ;
.

odrl:adHocShare
    schema:description "The act of sharing the asset to parties in close proximity to the owner."@en ;
    schema:name "Ad-hoc sharing"@en ;
.

odrl:aggregate
    schema:description "To use the Asset or parts of it as part of a composite collection."@en ;
    schema:name "Aggregate"@en ;
.

odrl:and
    schema:description "The relation is satisfied when all of the Constraints are satisfied."@en ;
    schema:name "And"@en ;
.

odrl:andSequence
    schema:description "The relation is satisfied when each of the Constraints are satisfied in the order specified."@en ;
    schema:name "And Sequence"@en ;
.

odrl:annotate
    schema:description "To add explanatory notations/commentaries to the Asset without modifying the Asset in any other way."@en ;
    schema:name "Annotate"@en ;
.

odrl:anonymize
    schema:description "To anonymize all or parts of the Asset."@en ;
    schema:name "Anonymize"@en ;
.

odrl:append
    schema:description "The act of adding to the end of an asset."@en ;
    schema:name "Append"@en ;
.

odrl:appendTo
    schema:description "The act of appending data to the Asset without modifying the Asset in any other way."@en ;
    schema:name "Append To"@en ;
.

odrl:archive
    schema:description "To store the Asset (in a non-transient form)."@en ;
    schema:name "Archive"@en ;
.

odrl:assignee
    schema:description "The Party is the recipient of the Rule."@en ;
    schema:name "Assignee"@en ;
.

odrl:assigneeOf
    schema:description "Identifies an ODRL Policy for which the identified Party undertakes the assignee functional role."@en ;
    schema:name "Assignee Of"@en ;
.

odrl:assigner
    schema:description "The Party is the issuer of the Rule."@en ;
    schema:name "Assigner"@en ;
.

odrl:assignerOf
    schema:description "Identifies an ODRL Policy for which the identified Party undertakes the assigner functional role."@en ;
    schema:name "Assigner Of"@en ;
.

odrl:attachPolicy
    schema:description "The act of keeping the policy notice with the asset."@en ;
    schema:name "Attach policy"@en ;
.

odrl:attachSource
    schema:description "The act of attaching the source of the asset and its derivatives."@en ;
    schema:name "Attach source"@en ;
.

odrl:attribute
    schema:description "To attribute the use of the Asset."@en ;
    schema:name "Attribute"@en ;
.

odrl:attributedParty
    schema:description "The Party to be attributed."@en ;
    schema:name "Attributed Party"@en ;
.

odrl:attributingParty
    schema:description "The Party who undertakes the attribution."@en ;
    schema:name "Attributing Party"@en ;
.

odrl:commercialize
    schema:description "The act of using the asset in a business environment."@en ;
    schema:name "Commercialize"@en ;
.

odrl:compensate
    schema:description "To compensate by transfer of some amount of value, if defined, for using or selling the Asset."@en ;
    schema:name "Compensate"@en ;
.

odrl:compensatedParty
    schema:description "The Party is the recipient of the compensation."@en ;
    schema:name "Compensated Party"@en ;
.

odrl:compensatingParty
    schema:description "The Party that is the provider of the compensation."@en ;
    schema:name "Compensating Party"@en ;
.

odrl:concurrentUse
    schema:description "To create multiple copies of the Asset that are being concurrently used."@en ;
    schema:name "Concurrent Use"@en ;
.

odrl:conflict
    schema:description "The conflict-resolution strategy for a Policy."@en ;
    schema:name "Handle Policy Conflicts"@en ;
.

odrl:consentedParty
    schema:description "The Party who obtains the consent."@en ;
    schema:name "Consented Party"@en ;
.

odrl:consentingParty
    schema:description "The Party to obtain consent from."@en ;
    schema:name "Consenting Party"@en ;
.

odrl:consequence
    schema:description "Relates a Duty to another Duty, the latter being a consequence of not fulfilling the former."@en ;
    schema:name "Consequence"@en ;
.

odrl:constraint
    schema:description "Constraint applied to a Rule"@en ;
    schema:name "Has Constraint"@en ;
.

odrl:contractedParty
    schema:description "The Party who is being contracted."@en ;
    schema:name "Contracted Party"@en ;
.

odrl:contractingParty
    schema:description "The Party who is offering the contract."@en ;
    schema:name "Contracting Party"@en ;
.

odrl:copy
    schema:description "The act of making an exact reproduction of the asset."@en ;
    schema:name "Copy"@en ;
.

odrl:core
    schema:description "Identifier for the ODRL Core Profile"@en ;
    schema:name "ODRL Core Profile"@en ;
.

odrl:count
    schema:description "Numeric count of executions of the action of the Rule."@en ;
    schema:name "Count"@en ;
.

odrl:dataType
    schema:description "The datatype of the value of the rightOperand or rightOperandReference of a Constraint."@en ;
    schema:name "Datatype"@en ;
.

odrl:dateTime
    schema:description "The date (and optional time and timezone) of exercising the action of the Rule. Right operand value MUST be an xsd:date or xsd:dateTime as defined by [[xmlschema11-2]]."@en ;
    schema:name "Datetime"@en ;
.

odrl:delayPeriod
    schema:description "A time delay period prior to exercising the action of the Rule. The point in time triggering this period MAY be defined by another temporal Constraint combined by a Logical Constraint (utilising the odrl:andSequence operand). Right operand value MUST be an xsd:duration as defined by [[xmlschema11-2]]."@en ;
    schema:name "Delay Period"@en ;
.

odrl:delete
    schema:description "To permanently remove all copies of the Asset after it has been used."@en ;
    schema:name "Delete"@en ;
.

odrl:deliveryChannel
    schema:description "The delivery channel used for exercising the action of the Rule."@en ;
    schema:name "Delivery Channel"@en ;
.

odrl:derive
    schema:description "To create a new derivative Asset from this Asset and to edit or modify the derivative."@en ;
    schema:name "Derive"@en ;
.

odrl:device
    schema:description "An identified device used for exercising the action of the Rule."@en ;
    schema:name "Device"@en ;
.

odrl:digitize
    schema:description "To produce a digital copy of (or otherwise digitize) the Asset from its analogue form."@en ;
    schema:name "Digitize"@en ;
.

odrl:display
    schema:description "To create a static and transient rendition of an Asset."@en ;
    schema:name "Display"@en ;
.

odrl:distribute
    schema:description "To supply the Asset to third-parties."@en ;
    schema:name "Distribute"@en ;
.

odrl:duty
    schema:description "Relates an individual Duty to a Permission."@en ;
    schema:name "Has Duty"@en ;
.

odrl:elapsedTime
    schema:description "A continuous elapsed time period which may be used for exercising of the action of the Rule. Right operand value MUST be an xsd:duration as defined by [[xmlschema11-2]]."@en ;
    schema:name "Elapsed Time"@en ;
.

odrl:ensureExclusivity
    schema:description "To ensure that the Rule on the Asset is exclusive."@en ;
    schema:name "Ensure Exclusivity"@en ;
.

odrl:eq
    schema:description "Indicating that a given value equals the right operand of the Constraint."@en ;
    schema:name "Equal to"@en ;
.

odrl:event
    schema:description "An identified event setting a context for exercising the action of the Rule."@en ;
    schema:name "Event"@en ;
.

odrl:execute
    schema:description "To run the computer program Asset."@en ;
    schema:name "Execute"@en ;
.

odrl:export
    schema:description "The act of transforming the asset into a new form."@en ;
    schema:name "Export"@en ;
.

odrl:extract
    schema:description "To extract parts of the Asset and to use it as a new Asset."@en ;
    schema:name "Extract"@en ;
.

odrl:extractChar
    schema:description "The act of extracting (replicating) unchanged characters from the asset."@en ;
    schema:name "Extract character"@en ;
.

odrl:extractPage
    schema:description "The act of extracting (replicating) unchanged pages from the asset."@en ;
    schema:name "Extract page"@en ;
.

odrl:extractWord
    schema:description "The act of extracting (replicating) unchanged words from the asset."@en ;
    schema:name "Extract word"@en ;
.

odrl:failure
    schema:description "Failure is an abstract property that defines the violation (or unmet) relationship between Rules."@en ;
    schema:name "Failure"@en ;
.

odrl:fileFormat
    schema:description "A transformed file format of the target Asset."@en ;
    schema:name "File Format"@en ;
.

odrl:function
    schema:description "Function is an abstract property whose sub-properties define the functional roles which may be fulfilled by a party in relation to a Rule."@en ;
    schema:name "Function"@en ;
.

odrl:give
    schema:description "To transfer the ownership of the Asset to a third party without compensation and while deleting the original asset."@en ;
    schema:name "Give"@en ;
.

odrl:grantUse
    schema:description "To grant the use of the Asset to third parties."@en ;
    schema:name "Grant Use"@en ;
.

odrl:gt
    schema:description "Indicating that a given value is greater than the right operand of the Constraint."@en ;
    schema:name "Greater than"@en ;
.

odrl:gteq
    schema:description "Indicating that a given value is greater than or equal to the right operand of the Constraint."@en ;
    schema:name "Greater than or equal to"@en ;
.

odrl:hasPart
    schema:description "A set-based operator indicating that a given value contains the right operand of the Constraint."@en ;
    schema:name "Has part"@en ;
.

odrl:hasPolicy
    schema:description "Identifies an ODRL Policy for which the identified Asset is the target Asset to all the Rules."@en ;
    schema:name "Target Policy"@en ;
.

odrl:ignore
    schema:description "The Action is to be ignored and is not part of the policy – and the policy remains valid."@en ;
    schema:name "Ignore Undefined Actions"@en ;
.

odrl:implies
    schema:description "An Action asserts that another Action is not prohibited to enable its operational semantics."@en ;
    schema:name "Implies"@en ;
.

odrl:include
    schema:description "To include other related assets in the Asset."@en ;
    schema:name "Include"@en ;
.

odrl:includedIn
    schema:description "An Action transitively asserts that another Action that encompasses its operational semantics."@en ;
    schema:name "Included In"@en ;
.

odrl:index
    schema:description "To record the Asset in an index."@en ;
    schema:name "Index"@en ;
.

odrl:industry
    schema:description "A defined industry sector setting a context for exercising the action of the Rule."@en ;
    schema:name "Industry Context"@en ;
.

odrl:inform
    schema:description "To inform that an action has been performed on or in relation to the Asset."@en ;
    schema:name "Inform"@en ;
.

odrl:informedParty
    schema:description "The Party to be informed of all uses."@en ;
    schema:name "Informed Party"@en ;
.

odrl:informingParty
    schema:description "The Party who provides the inform use data."@en ;
    schema:name "Informing Party"@en ;
.

odrl:inheritAllowed
    schema:description "Indicates if the Policy entity can be inherited."@en ;
    schema:name "Inheritance Allowed"@en ;
.

odrl:inheritFrom
    schema:description "Relates a (child) policy to another (parent) policy from which terms are inherited."@en ;
    schema:name "Inherits From"@en ;
.

odrl:inheritRelation
    schema:description "Indentifies the type of inheritance."@en ;
    schema:name "Inherit Relation"@en ;
.

odrl:install
    schema:description "To load the computer program Asset onto a storage device which allows operating or running the Asset."@en ;
    schema:name "Install"@en ;
.

odrl:invalid
    schema:description "The policy is void."@en ;
    schema:name "Void Policy"@en ;
.

odrl:isA
    schema:description "A set-based operator indicating that a given value is an instance of the right operand of the Constraint."@en ;
    schema:name "Is a"@en ;
.

odrl:isAllOf
    schema:description "A set-based operator indicating that a given value is all of the right operand of the Constraint."@en ;
    schema:name "Is all of"@en ;
.

odrl:isAnyOf
    schema:description "A set-based operator indicating that a given value is any of the right operand of the Constraint."@en ;
    schema:name "Is any of"@en ;
.

odrl:isNoneOf
    schema:description "A set-based operator indicating that a given value is none of the right operand of the Constraint."@en ;
    schema:name "Is none of"@en ;
.

odrl:isPartOf
    schema:description "A set-based operator indicating that a given value is contained by the right operand of the Constraint."@en ;
    schema:name "Is part of"@en ;
.

odrl:language
    schema:description "A natural language used by the target Asset."@en ;
    schema:name "Language"@en ;
.

odrl:lease
    schema:description "The act of making available the asset to a third-party for a fixed period of time with exchange of value."@en ;
    schema:name "Lease"@en ;
.

odrl:leftOperand
    schema:description "The left operand in a constraint expression."@en ;
    schema:name "Has Left Operand"@en ;
.

odrl:lend
    schema:description "The act of making available the asset to a third-party for a fixed period of time without exchange of value."@en ;
    schema:name "Lend"@en ;
.

odrl:license
    schema:description "The act of granting the right to use the asset to a third-party."@en ;
    schema:name "License"@en ;
.

odrl:lt
    schema:description "Indicating that a given value is less than the right operand of the Constraint."@en ;
    schema:name "Less than"@en ;
.

odrl:lteq
    schema:description "Indicating that a given value is less than or equal to the right operand of the Constraint."@en ;
    schema:name "Less than or equal to"@en ;
.

odrl:media
    schema:description "Category of a media asset setting a context for exercising the action of the Rule."@en ;
    schema:name "Media Context"@en ;
.

odrl:meteredTime
    schema:description "An accumulated amount of one to many metered time periods which were used for exercising the action of the Rule. Right operand value MUST be an xsd:duration as defined by [[xmlschema11-2]]."@en ;
    schema:name "Metered Time"@en ;
.

odrl:modify
    schema:description "To change existing content of the Asset. A new asset is not created by this action."@en ;
    schema:name "Modify"@en ;
.

odrl:move
    schema:description "To move the Asset from one digital location to another including deleting the original copy."@en ;
    schema:name "Move"@en ;
.

odrl:neq
    schema:description "Indicating that a given value is not equal to the right operand of the Constraint."@en ;
    schema:name "Not equal to"@en ;
.

odrl:nextPolicy
    schema:description "To grant the specified Policy to a third party for their use of the Asset."@en ;
    schema:name "Next Policy"@en ;
.

odrl:obligation
