PREFIX odrl: <http://www.w3.org/ns/odrl/2/>
PREFIX schema: <https://schema.org/>

<http://creativecommons.org/ns#Attribution>
    schema:description "Credit be given to copyright holder and/or author."@en ;
    schema:name "Attribution"@en ;
.

<http://creativecommons.org/ns#CommercialUse>
    schema:description "Exercising rights for commercial purposes."@en ;
    schema:name "Commercial Use"@en ;
.

<http://creativecommons.org/ns#DerivativeWorks>
    schema:description "Distribution of derivative works."@en ;
    schema:name "Derivative Works"@en ;
.

<http://creativecommons.org/ns#Distribution>
    schema:description "Distribution, public display, and publicly performance."@en ;
    schema:name "Distribution"@en ;
.

<http://creativecommons.org/ns#Notice>
    schema:description "Copyright and license notices be kept intact."@en ;
    schema:name "Notice"@en ;
.

<http://creativecommons.org/ns#Reproduction>
    schema:description "Making multiple copies."@en ;
    schema:name "Reproduction"@en ;
.

<http://creativecommons.org/ns#ShareAlike>
    schema:description "Derivative works be licensed under the same terms or compatible terms as the original work."@en ;
    schema:name "Share Alike"@en ;
.

<http://creativecommons.org/ns#Sharing>
    schema:description "Permits commercial derivatives, but only non-commercial distribution."@en ;
    schema:name "Sharing"@en ;
.

<http://creativecommons.org/ns#SourceCode>
    schema:description "Source code (the preferred form for making modifications) must be provided when exercising some rights granted by the license."@en ;
    schema:name "Source Code"@en ;
.

odrl:
    schema:description
        "The ODRL Vocabulary and Expression defines a set of concepts and terms (the vocabulary) and encoding mechanism (the expression) for permissions and obligations statements describing digital content usage based on the ODRL Information Model."@en ,
