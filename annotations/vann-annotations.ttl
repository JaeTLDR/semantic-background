PREFIX schema: <https://schema.org/>
PREFIX vann: <http://purl.org/vocab/vann/>

vann:
    schema:name "VANN: A vocabulary for annotating vocabulary descriptions"@en ;
.

vann:changes
    schema:description "A reference to a resource that describes changes between this version of a vocabulary and the previous."@en ;
    schema:name "Changes"@en ;
.

vann:example
    schema:description "A reference to a resource that provides an example of how this resource can be used."@en ;
    schema:name "Example"@en ;
.

vann:preferredNamespacePrefix
    schema:description "The preferred namespace prefix to use when using terms from this vocabulary in an XML document."@en ;
    schema:name "Preferred Namespace Prefix"@en ;
.

vann:preferredNamespaceUri
    schema:description "The preferred namespace URI to use when using terms from this vocabulary in an XML document."@en ;
    schema:name "Preferred Namespace Uri"@en ;
.

vann:termGroup
    schema:description "A group of related terms in a vocabulary."@en ;
    schema:name "Term Group"@en ;
.

vann:usageNote
    schema:description "A reference to a resource that provides information on how this resource is to be used."@en ;
    schema:name "Usage Note"@en ;
.
